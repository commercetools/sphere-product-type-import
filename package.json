{
  "name": "sphere-product-type-import",
  "version": "0.0.0-semantically-released",
  "description": "Library that helps with importing product type into the commeretools platform",
  "main": "dist/index.js",
  "scripts": {
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "commit": "git-cz",
    "test": "mocha tests/**/*.js --compilers js:babel-register",
    "test:ci": " npm run clean && babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- tests/**/*.js",
    "clean": "rimraf dist/* coverage",
    "test:watch": "watch 'npm test' src/ tests/",
    "build": "npm run clean && babel src --out-dir dist",
    "lint": "./scripts/lint src tests",
    "semantic-release": "semantic-release pre && npm run build && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sphereio/sphere-product-type-import.git"
  },
  "bugs": {
    "url": "https://github.com/sphereio/sphere-product-type-import/issues"
  },
  "keywords": [
    "ctp",
    "commercetools",
    "product-type",
    "import"
  ],
  "author": "Philipp Sporrer <philipp.sporrer@commercetools.de>",
  "homepage": "https://github.com/sphereio/sphere-product-type-import#readme",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.6.5",
    "babel-istanbul": "^0.6.1",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.9.2",
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.6.1",
    "cuid": "^1.3.8",
    "cz-conventional-changelog": "^1.1.5",
    "eslint-config-commercetools": "^3.2.1",
    "ghooks": "^1.0.3",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.3",
    "watch": "^0.17.1"
  },
  "dependencies": {
    "ajv": "^4.0.0",
    "bluebird": "^2.11.0",
    "lodash": "^4.6.1",
    "sphere-node-sdk": "^1.10.1",
    "sphere-node-utils": "^0.8.0",
    "underscore": "^1.8.3",
    "underscore-mixins": "^0.1.4"
  },
  "eslintConfig": {
    "extends": "commercetools"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:ci && npm run check-coverage"
    }
  }
}
